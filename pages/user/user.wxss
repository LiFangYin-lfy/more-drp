page {
  background-color: #f7f8fa;
}
.container {
  width: 100%;
}
.container .bigboxs {
  width: 100%;
  background: linear-gradient(133deg, #f7bf00, #ed8b00 98%);
}
.container .bigboxs .header {
  width: 100%;
}
.container .bigboxs .header .title {
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #fff;
}
.container .bigboxs .userinfo {
  width: 100%;
  padding: 20rpx 32rpx 0rpx;
  box-sizing: border-box;
}
.container .bigboxs .userinfo .tophead {
  width: 100%;
  display: flex;
  align-items: center;
}
.container .bigboxs .userinfo .tophead .left_head {
  flex: 1;
  display: flex;
  align-items: center;
}
.container .bigboxs .userinfo .tophead .left_head image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.container .bigboxs .userinfo .tophead .left_head .head_name {
  padding-left: 24rpx;
  box-sizing: border-box;
  font-size: 36rpx;
  color: #fff;
}
.container .bigboxs .userinfo .tophead .right_head {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.container .bigboxs .userinfo .tophead .right_head .student {
  background: linear-gradient(313deg, #ffba6a 4%, #ffca7f 58%, #f8d59b 87%, #ffe5b7);
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.container .bigboxs .userinfo .tophead .right_head .student .stu_name {
  font-size: 28rpx;
  color: #995822;
}
.container .bigboxs .userinfo .tophead .right_head .student image {
  width: 20rpx;
  height: 24rpx;
  margin-left: 20rpx;
}
.container .bigboxs .userinfo .rules {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.container .bigboxs .userinfo .rules image {
  width: 32rpx;
  height: 32rpx;
}
.container .bigboxs .userinfo .rules .rules_name {
  margin-left: 16rpx;
  font-size: 28rpx;
  color: #fff;
}
.container .bigboxs .userinfo .pop {
  width: 100%;
  background: linear-gradient(313deg, #ffba6a 4%, #ffca7f 58%, #f8d59b 87%, #ffe5b7);
  border-radius: 16rpx 16rpx 0 0;
  display: flex;
  align-items: center;
  padding: 32rpx 0;
  box-sizing: border-box;
}
.container .bigboxs .userinfo .pop .felx {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container .bigboxs .userinfo .pop .felx .number {
  font-size: 38rpx;
  color: #965620;
}
.container .bigboxs .userinfo .pop .felx .number_name {
  font-size: 28rpx;
  color: #965620;
}
.container .bigboxs .userinfo .pop .felx2 {
  border-right: 2rpx solid #965620;
}
.container .order {
  width: 100%;
  padding: 20rpx 32rpx 20rpx;
  box-sizing: border-box;
}
.container .order .banner {
  width: 100%;
  background-color: #fff;
  height: 240rpx;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
}
.container .order .banner image {
  width: 100%;
  border-radius: 16rpx;
}
.container .order .order_list {
  width: 100%;
  background-color: #fff;
  border-radius: 16rpx;
}
.container .order .order_list .title_order {
  width: 100%;
  background-color: #fff;
  padding: 32rpx;
  box-sizing: border-box;
  font-weight: bold;
  border-radius: 16rpx 16rpx 0 0;
}
.container .order .order_list .order_cun {
  width: 100%;
  display: flex;
  align-items: center;
  padding-bottom: 32rpx;
}
.container .order .order_list .order_cun .box1 {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
}
.container .order .order_list .order_cun .box1 image {
  width: 76rpx;
  height: 72rpx;
  flex: 0 auto;
}
.container .order .order_list .order_cun .box1 .b_name {
  padding: 10rpx 0;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333333;
}
.container .order .order_list .order_cun .box1 .count {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background-color: #FF742F;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  position: absolute;
  top: -10rpx;
  right: 25rpx;
  font-size: 24rpx;
}
.container .order .downlise {
  width: 100%;
  margin-top: 20rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 16rpx;
}
.container .order .downlise .listitem {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 32rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f9f9f9;
}
.container .order .downlise .listitem .li_img {
  display: flex;
  align-items: center;
}
.container .order .downlise .listitem .li_img image {
  width: 48rpx;
  height: 48rpx;
}
.container .order .downlise .listitem .li_img .li_name {
  margin-left: 24rpx;
  font-size: 32rpx;
  color: #333333;
}
.container .order .downlise .listitem .arrowimg {
  width: 24rpx;
  height: 24rpx;
}
