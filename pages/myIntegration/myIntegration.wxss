page {
  background-color: #f7f8fa;
}
.container {
  width: 100%;
  padding-bottom: 30rpx;
}
.container .one_list {
  width: 100%;
}
.container .one_list .one_title {
  width: 100%;
}
.container .one_list .one_title .title_names {
  width: 100%;
  font-size: 28rpx;
  color: #999999;
  padding: 32rpx;
  box-sizing: border-box;
}
.container .one_list .one_title .two_item {
  width: 100%;
  background-color: #fff;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.container .one_list .one_title .two_item .itemList {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 32rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f9f9f9;
}
.container .one_list .one_title .two_item .itemList .itemname {
  font-size: 32rpx;
  color: #323233;
}
.container .one_list .one_title .two_item .itemList .itemcount {
  font-size: 32rpx;
  color: #FF742F;
}
.container .cno {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .cno .kongbox {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 200rpx auto;
}
.container .cno .kongbox image {
  width: 100%;
  height: 450rpx;
}
.container .cno .kongbox .c_name {
  margin-top: 30rpx;
  font-size: 32rpx;
  color: #ccc;
}
